% wni, sample


# If you select 2 for x, the possible values of y will be 12 and 22
echo <x1> <y1>


$ x1: echo -e '1\n2\n3' | tr '\n' ' '
$ y1: echo "$((x1+10))" "$((x1+20))" | tr '\n' ' '

$ jtime: echo -e '3d\n2w\n1m'

# navi fzf install for new user account
su -c "/opt/help/fzf/install --key-bindings      --completion      --update-rc   &&      source ~/.bashrc"  <<linux_user_name>>

